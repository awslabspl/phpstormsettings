<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*.conf;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc;" />
    <extensionMap>
      <mapping pattern=".markdownlint" type="JSON" />
      <mapping pattern=".reverted" type="Markdown" />
      <removed_mapping ext="scss" approved="true" type="CSS" />
      <removed_mapping ext="sql" approved="true" type="DBN-SQL" />
    </extensionMap>
  </component>
</application>